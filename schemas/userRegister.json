    "required": [
        "username",
        "password",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "username"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "password"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "format": "email",
            "examples": [
                "email@email.com"
            ]
        }
    },
    "examples": [{
        "username": "username",
        "password": "password",
        "email": "email@email.com"
    }]
