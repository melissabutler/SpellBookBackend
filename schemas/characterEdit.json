{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "char_name",
        "char_class",
        "lvl"
    ],
    "properties": {
        "char_name": {
            "type": "string",
            "default": "",
            "title": "The char_name Schema",
            "examples": [
                "characterName"
            ]
        },
        "char_class": {
            "type": "string",
            "default": "",
            "title": "The char_class Schema",
            "examples": [
                "characterClass"
            ]
        },
        "lvl": {
            "type": "integer",
            "default": 0,
            "title": "The lvl Schema",
            "examples": [
                1
            ]
        },
        "strength": {
            "type": "integer",
            "default": 10,
            "title": "the Strength Schema"
        },
        
        "dexterity": {
            "type": "integer",
            "default": 10,
            "title": "the dexterity Schema"
        },
        "constitution": {
            "type": "integer",
            "default": 10,
            "title": "the constitution Schema"
        },
        "intelligence": {
            "type": "integer",
            "default": 10,
            "title": "the intelligence Schema"
        },
        "wisdom": {
            "type": "integer",
            "default": 10,
            "title": "the wisdom Schema"
        },
        "charisma": {
            "type": "integer",
            "default": 10,
            "title": "the charisma Schema"
        }
    },
    "examples": [{
        "char_name": "characterName",
        "char_class": "characterClass",
        "lvl": 1
    }]
}